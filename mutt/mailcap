# HTML
text/calendar; /usr/bin/firefox %s;
text/html; /usr/bin/firefox %s;
text/html; lynx -dump -assume_charset=%{charset} -display_charset=utf-8 %s; copiousoutput; nametemplate=%s.html

# Images
